<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0"> 
    <link rel="stylesheet" href="/css/saved_recipe.css">
    <title>Saved</title>
</head>
    <body> 
        <header>
            <div class="header_item">
                <p>Saved Recipe</p> 
            </div> 
            <div class="header_item_nav">
                <a id="saved_recipe" href="/">Back</a>
            </div>
        </header>
       <% recipes.forEach((item) => { %>    
        <section class="main"> 
            <section class="main_recipe"> 
                <div class="main_recipe_item_title">
                    <p><%= item.title %></p>   
                </div>  
                <div class="main_recipe_item">  

                    <form action="/delete/<%= item.url_id %>" method="POST"> 
                        <input id="read" type="submit" value="Delete">
                    </form> 

                    <form action="/food/<%= item.url_title %>" method="POST"> 
                        <input id="read" type="submit" value="Read"> 
                    </form>     

                </div>
            </section>
        </section>
       <% }) %> 
       
    </body>
</html>